-- also there's a copy (?) at 0x3...
base = 0x4301e8000
i4be = '4 Byte Big Endian'

al = getAddressList()
offset = 0

local function addOffset(n)
  offset = offset + n
end

local function addGroupRecord(pmr, name)
  local rec = al.createMemoryRecord()
  rec.setDescription(name)
  rec.isGroupHeader = true
  rec.Options = 'moHideChildren'
  rec.appendToEntry(pmr)
  return rec
end

local function addValueRecord(pmr, name)
  local rec = al.createMemoryRecord()
  rec.setDescription(name)
  rec.setAddress(base + offset)
  rec.Type = vtCustom
  rec.CustomTypeName = i4be
  rec.appendToEntry(pmr)
  addOffset(4)
  return rec
end

local pmr = al.getMemoryRecordByDescription('Player')
offset = 0x2c

local function addValueGroup(pmr, name)
  local v = addGroupRecord(pmr, name)
  addValueRecord(v, 'Current ' .. name)
  addValueRecord(v, 'Max ' .. name .. ' without Humanity')
  addValueRecord(v, 'Max ' .. name)
end

addValueGroup(pmr, 'HP')
addValueGroup(pmr, 'MP')
addOffset(4)
addValueGroup(pmr, 'Stamina')

local function addValueGroup(pmr, name)
  local v = addGroupRecord(pmr, name)
  addValueRecord(v, 'Current ' .. name)
  addValueRecord(v, 'Max ' .. name)
end

local smr = addGroupRecord(pmr, 'Stats')
offset = 0x58
addValueGroup(smr, 'Vitality')
addValueGroup(smr, 'Intellect')
addValueGroup(smr, 'Endurance')
addValueGroup(smr, 'Strength')
addValueGroup(smr, 'Agility')
addValueGroup(smr, 'Magic')
addValueGroup(smr, 'Hope')
addValueGroup(smr, 'Luck')

addValueRecord(pmr, 'Souls')

---------
-- Items
---------

-- first item is at 0x558 offset
item_offset = 0x558

local function addItem(pmr, name)
  addOffset(4)
  local idmr = addValueRecord(pmr, 'Item ID')
  local q = addValueRecord(pmr, 'Quantity')
  addOffset(0x20 - 0xC)

  id = tonumber(idmr.Value)
  memoryrecord_delete(idmr)

  if id == -1 or id == 4294967295 then
     memoryrecord_delete(q)
     return
  end
  if t[id] == nil then
     q.Description = 'Item #' .. tostring(id)
  else
     q.Description = t[id]
  end
end

local imr = addGroupRecord(pmr, 'Items')

offset = item_offset
for var=0, 0x47, 1 do
    addItem(imr, "Item #" .. tostring(var))
end

---------
-- All Items
---------

local function addItem(pmr, name)
  local v = addGroupRecord(pmr, name)
  addValueRecord(v, 'Unk0')
  local id = addValueRecord(v, 'Item ID')
  addValueRecord(v, 'Quantity')
  addValueRecord(v, 'Inventory ID')
  addOffset(0x20 - 0x10)
  v.Description = t[tonumber(id.Value)]
end

local imr = addGroupRecord(pmr, 'All Items')

offset = item_offset
for var=0, 0x47, 1 do
    addItem(imr, "Item #" .. tostring(var))
end

