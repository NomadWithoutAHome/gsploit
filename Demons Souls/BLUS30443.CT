aobScan(healthWriteAob,46 ?? ?? ?? C4 03 00 00 48 ?? ?? ?? ?? ?? ?? ?? ?? ?? ?? ?? ?? ?? ?? ?? BA ?? ?? ?? ?? E9 ?? ?? ?? ?? 0F 1F 00)
registerSymbol(healthWriteAob)

alloc(healthWriteCave,1024)
label(healthWriteCode)
registerSymbol(healthWriteCode)
label(healthWriteCleanup)
label(healthWriteRet)

alloc(chrInsBase,8)
registerSymbol(chrInsBase)

healthWriteCave:
push r9
mov r9,rax
add r9,r8
mov [chrInsBase],r9
jmp healthWriteCleanup

healthWriteCleanup:
pop r9
jmp healthWriteCode

healthWriteCode:
readMem(healthWriteAob,8)
jmp healthWriteRet

healthWriteAob:
jmp healthWriteCave
nop 3
healthWriteRet:

[disable]
healthWriteAob:
readMem(healthWriteCode,8)

deAlloc(chrInsBase)
deAlloc(healthWriteCave)
unregisterSymbol(chrInsBase)
unregisterSymbol(healthWriteCode)
unregisterSymbol(healthWriteAob)

